{
  "name": "ollama-chat",
  "version": "1.0.0",
  "description": "Simple chat client for Ollama",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "dev": "nodemon src/index.ts",
    "start:falcon": "OLLAMA_MODEL=falcon ts-node src/index.ts",
    "start:mistral": "OLLAMA_MODEL=mistral ts-node src/index.ts",
    "start:mathstral": "OLLAMA_MODEL=mathstral ts-node src/index.ts",
    "start:tinyllama": "OLLAMA_MODEL=tinyllama ts-node src/index.ts",
    "start:llama2": "OLLAMA_MODEL=llama2 ts-node src/index.ts",
    "start:codellama": "OLLAMA_MODEL=codellama ts-node src/index.ts",
    "start:custom": "ts-node src/index.ts"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "node-cache": "^5.1.2",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/node": "^20.10.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
